<h1>Listing coordenadas</h1>

<table>
  <tr>
    <th>Coord x</th>
    <th>Coord y</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coordenadas.each do |coordenada| %>
  <tr>
    <td><%= coordenada.coord_x %></td>
    <td><%= coordenada.coord_y %></td>
    <td><%= link_to 'Show', coordenada %></td>
    <td><%= link_to 'Edit', edit_coordenada_path(coordenada) %></td>
    <td><%= link_to 'Destroy', coordenada, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Coordenada', new_coordenada_path %>
