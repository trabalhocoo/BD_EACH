<h2>Sugestões</h2>
	<table id="tabela_sugestoes"  align="center">
			<tr class="imagem_sugestao_titulo">
				<td>Atributo</td>
				<td>Sugestão</td>
				<td>Score</td>
				<td>sugerido por<td>
			</tr>
			<% @suggestion.each do |suggestion| %>
				<tr>
					<td class="atributo_nome"><%= suggestion.atributo.nome %></td>
					<td><%= suggestion.sugestao %></td>
					<td><%= suggestion.pontuacao %></td>
					<td><%= suggestion.usuario.nome %></td>
					<td><%= render :partial => "voto_form", :locals => {:suggestion => suggestion}  %></td>
				</tr>
			<% end %>
	</table>
