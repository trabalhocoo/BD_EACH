<div id="templatemo_content">
  
  <table width="321" border="0" align="left" cellspacing="10">
   <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
   <%= devise_error_messages! %>
    <tr>
      <td width="279">
	<h2> Login  </h2>

        <h5>Email:</h5>
        <div><%= f.email_field :email %></div>

        <h5>Password:</h5>
        <div><%= f.password_field :password %></div>
      </td>
    </tr>

    <tr>
      <td><div><%= f.submit "Entrar" %></div></td>
    </tr>


    <tr>
      <td><div><h7><%= render :partial => "devise/shared/links" %></h7></div></td>
    </tr>	
    
    <% end %>

  </table>
</div>
